    "path-to-regexp": "2.4.0",
    "qs": "^6.7.0",
    "vue": "2.6.10",
    "vue-bus": "^1.2.1",
    "vue-count-to": "^1.0.13",
    "vue-image-swipe": "^1.0.5",
    "vue-lazyload": "^1.3.1",
